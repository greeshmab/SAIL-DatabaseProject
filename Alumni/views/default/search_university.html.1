{{extend 'layout.html'}}

<h3>Searching By name</h3>
<table class="table">
    {{for row in rows:}}
    <tr>
        <td><a href="{{=URL('show',args=row.id)}}"></a>{{=row.iitg_webmail_id}} </td>
        <td>{{=row.deg}  </td>
        <td>{{=row.dept}}    </td>
        <td>{{=row.univ}}      </td>
        <td>{{=row.start_year}}     </td>
        <td>{{=row.end_year}}          </td>
    </tr>
    {{pass}}
</table>
